    "jsdom": "^7.0.1",
    "mdast": "^2.1.0",
    "mocha": "^2.2.5",
    "nlcst-to-string": "^1.0.0",
    "node-fetch": "^1.3.2",
    "npm-run-all": "^1.2.8",
    "power-assert": "^1.0.0",
    "stemming-x-keywords": "^1.0.3",
    "textlint": "^5.2.0",
    "textlint-formatter-codecov": "^1.0.2",
    "textlint-rule-max-ten": "^2.0.0",
    "textlint-rule-no-double-negative-ja": "^1.0.2",
